	<SPRITE xxx x=4 y=0 z=1 bmp="$tmc_event_new.spr" phase=0 visiblemodel="!*.is_new">
    

	<SPRITE xxx x=290 y=4  z=1 bmp="$tmc_event_onroute.spr" phasemodel="*.on_route">	
