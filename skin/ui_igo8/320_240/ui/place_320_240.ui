	<TEXT txtMPTime4label template=txt_multi_label y=162 text="Time:" fontsize=10  align="LEFT" watchstate=btnMR_easy>
	<TEXT txtMPTime4      template=txt_multi_value y=162 textmodel_timespan="route.multiplan4.time" formatmodel=3 font="tahomabd" watchstate=btnMR_easy visiblemodel="route.multiplan4.time.valid">
	<TEXT txtMPTime4Unit  template=txt_multi_unit  y=162 textmodel_timespan="route.multiplan4.time" formatmodel=4 visiblemodel="route.multiplan4.time.valid">
	<ALIGNER xxx          template=aligner_multi   x=220 control=txtMPTime4label padding=5 control=txtMPTime4 padding=3 control=txtMPTime4Unit>
		
