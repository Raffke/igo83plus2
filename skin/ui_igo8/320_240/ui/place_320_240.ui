	<TEXT txtMPDist1      template=txt_multi_value y=27 textmodel_wstr="route.multiplan1.dist.number_text" watchstate=btnMR_fast visiblemodel="route.multiplan1.dist.valid">
	<TEXT txtMPDist1Unit  template=txt_multi_unit  y=27 textmodel_wstr="route.multiplan1.dist.unit_text" visiblemodel="route.multiplan1.dist.valid">
