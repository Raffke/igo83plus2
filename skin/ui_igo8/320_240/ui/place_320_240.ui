	<TEXT txtMPTime3label template=txt_multi_label y=117 text="Time:" watchstate=btnMR_econo>
	<TEXT txtMPTime3      template=txt_multi_value y=117 textmodel_timespan="route.multiplan3.time" formatmodel=3 watchstate=btnMR_econo visiblemodel="route.multiplan3.time.valid">
	<TEXT txtMPTime3Unit  template=txt_multi_unit  y=117 textmodel_timespan="route.multiplan3.time" formatmodel=4 watchstate=btnMR_econo visiblemodel="route.multiplan3.time.valid">
	<ALIGNER xxx          template=aligner_multi   x=220 control=txtMPTime3label padding=5 control=txtMPTime3 padding=3 control=txtMPTime3Unit>
	
	<BUTTON btnMR_easy    template=btn_4a          y=155 valign="TOP+6" font="tahomabd"  text="Easy" fontsize=12 onrelease='run sc_SelectMultiplanRoute 3'>
	<SPRITE sprMPRoute4   template=spr_multi       y=175>
