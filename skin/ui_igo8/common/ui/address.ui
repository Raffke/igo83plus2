	map.CITYSELECTED vSelectedUID "vIsCOS" "vIsHaveCOS" vSelectedAdvUID
	PREVSTATE
	runif $1 1 'PREVSTATE'
	RUNIF vIsCOS 1 sc_SearchCityCOSAndGo
	ELSE_RUNIF vIsCOS 2 sc_CityHaveOnlyHousenumber
</script>

