	lbFACR_CityResult.GETUID "vSelectedUID" 
	lbFACR_CityResult.GETADVUID "vSelectedAdvUID"
	
	vTmp.SET $1
	RUNIF vTmp 0 'vSelectedCity.SET L"All", vSelectedItem2.SET L"All"'
	RUNIF bFindDelayed 0 'run sc_StoreSelectedCity vTmp'
</script>

