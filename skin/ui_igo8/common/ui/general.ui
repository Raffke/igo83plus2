<script sc_StartInfoMessageBox>
	btnMB_Ok.show
	btnMB_Yes.hide
	btnMB_No.hide
    runif vUIres "800_480" 'sprMB_Icon.bmp "dialog_em.spr"'
    else_runif vUIres "480_800" 'sprMB_Icon.bmp "dialog_em.spr"'
