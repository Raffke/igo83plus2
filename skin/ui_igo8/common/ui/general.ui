	btnMB_No.show
    runif vUIres "800_480" 'sprMB_Icon.bmp "dialog_qm.spr"'
    else_runif vUIres "480_800" 'sprMB_Icon.bmp "dialog_qm.spr"'
    else_run 'sprMB_Icon.bmp "ui_igo8/common/dialog_qm.spr"'
</script>

<script sc_StartCustomMessageBox>
