	v3dzoomcontrol.set 0
	runif vHideZoomControls 0 'run sc_HideCockpitPanel, v3dzoomcontrol.set 1'
	else_runif vHideZoomControls 1 'v3dzoomcontrol.set 0, run sc_ShowCockpitPanel'
</script>

<script sc_Show2DZoomControls>
