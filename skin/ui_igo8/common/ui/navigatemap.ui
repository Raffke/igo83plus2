	else_runif nVehicle 4 'btnMR_vehicle.iconspr "$vhcl_emr_onbtn.bmp"'
	else_runif nVehicle 5 'btnMR_vehicle.iconspr "$vhcl_ped_onbtn.bmp"'
	else_runif nVehicle 6 'btnMR_vehicle.iconspr "$vhcl_bic_onbtn.bmp"'
	</script>

