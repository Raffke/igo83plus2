<script sc_btnCS_Mode_Cockpit_OnLongclick>
	runif vFullScreenCockpit 1 'NEXTSTATE st_FullScreenCockpit'
	else_run 'vMapSettingsActPage.set 2, NEXTSTATE st_SettingVisual'
</script>

