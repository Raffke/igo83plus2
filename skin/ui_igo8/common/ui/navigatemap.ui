	sprNMIActualHouseNum1.visiblemodel "navigation.car_pos_valid&map.follow_gps"
	sprNMIActualHouseNum2.visiblemodel "navigation.car_pos_valid&map.follow_gps"
	run sc_RD_ChangeNormalScreenMode

