<script sc_SpeedcamGetLangTypeValues>
;!Raffke_jr - added voices, offset 10 to sort foreign languages to end; add additional ascending
  vTmp.Set "Alert sound|-1|Off|0|German Female|1|German Male|2|Russian Male|11|Russian Female|12|UK English Female|13|German|4|Czech Male|15|Czech Female|16|Hebrew Male|17|Italian|18|Hungarian Male|19|Hungarian Female|20|French Female|21|Espanol Female|22|Romanian Female|23|Polish Female|24"
</script>

<script sc_SpeedcamVoiceTestSound>
      runif vSpeedcamWarningType      11 'PLAYSOUND "RUM_warning_1"'
      else_runif vSpeedcamWarningType 12 'PLAYSOUND "RU_warning_1"'
      else_runif vSpeedcamWarningType 13 'PLAYSOUND "EN_warning_1"'
      else_runif vSpeedcamWarningType  4 'PLAYSOUND "DE_einfach_1"'
      else_runif vSpeedcamWarningType 15 'PLAYSOUND "CSM_warning_1"'
      else_runif vSpeedcamWarningType 16 'PLAYSOUND "CSF_warning_1"'
      else_runif vSpeedcamWarningType 17 'PLAYSOUND "HE_warning_1"'
;!Raffke_jr - added Byte's voices, offset 50
      else_runif vSpeedcamWarningType  1 'PLAYSOUND "einfach_0_w"'
      else_runif vSpeedcamWarningType  2 'PLAYSOUND "einfach_0_m"'
;!Raffke_jr - added GlikoGliko's italian voices
      else_runif vSpeedcamWarningType 18 'PLAYSOUND "fisso"'
;!Raffke_jr - added Riks voices
      else_runif vSpeedcamWarningType 19 'PLAYSOUND "HU_warning_1"'
      else_runif vSpeedcamWarningType 20 'PLAYSOUND "HUF_warning_1"'
      else_runif vSpeedcamWarningType 21 'PLAYSOUND "RF-simple"'
      else_runif vSpeedcamWarningType 22 'PLAYSOUND "ES_warning_2"'
      else_runif vSpeedcamWarningType 23 'PLAYSOUND "RO_warning_0"'   
      else_runif vSpeedcamWarningType 24 'PLAYSOUND "PLF_warning_1"'
</script>

<script sc_spoken_alert_lang_select>
      runif vSpeedcamWarningType      11 'run sc_selection_alert_RUM'
      else_runif vSpeedcamWarningType 12 'run sc_selection_alert_RU'
      else_runif vSpeedcamWarningType 13 'run sc_selection_alert_EN'
      else_runif vSpeedcamWarningType  4 'run sc_selection_alert_DE'
      else_runif vSpeedcamWarningType 15 'run sc_selection_alert_CSM'
      else_runif vSpeedcamWarningType 16 'run sc_selection_alert_CSF'
      else_runif vSpeedcamWarningType 17 'run sc_selection_alert_HE'
;!Raffke_jr - added Byte's voices, offset 50
      else_runif vSpeedcamWarningType  1 'run sc_selection_alert_W'
      else_runif vSpeedcamWarningType  2 'run sc_selection_alert_M'
;!Raffke_jr - added GlikoGliko's italian voices
      else_runif vSpeedcamWarningType 18 'run sc_selection_alert_ITA'
;!Raffke_jr - added Riks voices
      else_runif vSpeedcamWarningType 19 'run sc_selection_alert_HU'
      else_runif vSpeedcamWarningType 20 'run sc_selection_alert_HUF'
      else_runif vSpeedcamWarningType 21 'run sc_selection_alert_FRA'
      else_runif vSpeedcamWarningType 22 'run sc_selection_alert_ESP'
      else_runif vSpeedcamWarningType 23 'run sc_selection_alert_RO'   
      else_runif vSpeedcamWarningType 20 'run sc_selection_alert_PLF'
</script>

<script sc_selection_alert_RUM>
