<layer ui_SpeedCam z=30>
	<BUTTON	btnSC_Type   template=btntext   x=10  y=40  z=0         bmp="$speedcam_bg.bmp#2" text="Camera Type" align="CENTER" valign="BOTTOM-10" onrelease='run sc_SelectCameraType'>
	<SPRITE sprSC_Type   template=btntext   x=10  y=40  z=1         bmp="$kamerak.spr">
	
	<SPRITE  txtSC_Poti								x=2    y=167  z=0        bmp="poti_bg.bmp">
	<TEXT   txtSC_Speed       template=txt_csu_legend  template=only_white y=167  text="Camera Speed:">
	<TEXT   txtSC_SpeedValue  template=txt_csu_value   template=only_white y=167  >
	<TEXT   txtSC_SpeedUnit   template=txt_csu_unit    template=only_white y=167  >
	<TEXT   txtSC_SpeedNo     template=txt_csu_valunit template=only_white y=167  text="No Limit" >
	<HSCROLLBAR hscSC_Speed   template=csuszka          y=200  endsize=0 possiblevalues="0, 20, 30, 40, 50, 60, 70, 80, 90, 100, 110, 120, 130" blockincrement=_0.0 onchange='run sc_hscSC_Speed_OnChange' endsize=15>
	
	<SPRITE     sprSC_Speed		template=spr_5a	y=-206>
	
	<BUTTON	btnSC_Dir    template=btntext   x=125 y=40 w=92 z=0         bmp="$speedcam_bg.bmp#2" text="Camera\nDirection" align="CENTER" valign="BOTTOM-10">
	<SPRITE sprSC_Dir                       x=176 y=75 z=1          bmp="ui_igo8/common/direct.spr">
	
	<BUTTON btnSC_Delete template=footer2_1a 	z=0	text="" iconspr="icof_deletecam.bmp" onlongclick='run sc_show_btn_help "Delete"' onrelease='run sc_btnSC_Delete_OnRelease'>
	<BUTTON xxx          template=footer2_2a	z=0	iconspr="icof_done.bmp#3"   onlongclick='run sc_show_btn_help "Ok"'     onrelease='run sc_SpeedCamOK'>
</layer>

<layer ui_SpeedCamType z=30>
	<BUTTON	btnSCT_Type1  template=btntext   x=15  y=30  z=0   bmp="$speedcamcell_bg.spr#2" iconspr="$kamerak.spr" iconphase=0 iconalign="CENTER" iconvalign="CENTER-6" onrelease='run sc_btnSCT_Type_OnClick 0'>
	<BUTTON	btnSCT_Type2  template=btntext   x=121 y=30  z=0   bmp="$speedcamcell_bg.spr#2" iconspr="$kamerak.spr" iconphase=1 iconalign="CENTER" iconvalign="CENTER-6" onrelease='run sc_btnSCT_Type_OnClick 1'>
	<BUTTON	btnSCT_Type3  template=btntext   x=15  y=110  z=0   bmp="$speedcamcell_bg.spr#2" iconspr="$kamerak.spr" iconphase=2 iconalign="CENTER" iconvalign="CENTER-6" onrelease='run sc_btnSCT_Type_OnClick 2'>
	<BUTTON	btnSCT_Type4  template=btntext   x=121 y=110  z=0   bmp="$speedcamcell_bg.spr#2" iconspr="$kamerak.spr" iconphase=3 iconalign="CENTER" iconvalign="CENTER-6" onrelease='run sc_btnSCT_Type_OnClick 3'>
	<BUTTON	btnSCT_Type5  template=btntext   x=15  y=190  z=0   bmp="$speedcamcell_bg.spr#2" iconspr="$kamerak.spr" iconphase=4 iconalign="CENTER" iconvalign="CENTER-6" onrelease='run sc_btnSCT_Type_OnClick 4'>
</layer>

<layer ui_SpeedCamWarningLayer z=5000 visiblemodel="navigation.curr_speedcam.category.valid&warning.speedcam_visualtype!=0&!ui.vOnFlyover">
	<SPRITE xxx x=75 y=20 z=0 h=65 bmp="cock_infobg2.bmp" visiblemodel="!ui.v3dzoomcontrol">

	<SPRITE sprSCW_CamType       x=87 y=21 z=1  phasemodel="navigation.curr_speedcam.category" bmp="$speed_cams.spr">
	<SPRITE sprSCW_CamEnterExit  x=88 y=23 z=2  phasemodel="navigation.curr_speedcam.enterexit" bmp="$speed_cam_enterexit.spr">

	<SPRITE xxx x=5  y=255 z=50 bmp="$speed_cam_speedlimit.bmp">
	<TEXT   xxx x=11 y=255 w=28 h=40 z=51 textmodel_wstr="navigation.curr_speedcam.speed_limit.number_text" visiblemodel="navigation.curr_speedcam.speed_limit.valid"  align="CENTER" fontsize=18 font="tahomabd" color=#000000>
	<TEXT   xxx x=11 y=255 w=28 h=40 z=51 text="!"                                                          visiblemodel="!navigation.curr_speedcam.speed_limit.valid" align="CENTER" fontsize=18 font="tahomabd" color=#000000>

;!Raffke_jr - 2x removed align
	<TEXT    txtSCW_AvgSpeed     template=txt_sec textmodel_wstr="navigation.curr_speedcam.avg_speed.number_text" visiblemodel="navigation.curr_speedcam.avg_speed.valid" fontsize=12>
	<TEXT    txtSCW_AvgSpeedUnit template=txt_sec textmodel_wstr="navigation.curr_speedcam.avg_speed.unit_text"   visiblemodel="navigation.curr_speedcam.avg_speed.valid" fontsize=8 >
	<ALIGNER xxx                 x=80  w=58  align="LEFT" control=txtSCW_AvgSpeed padding=4 control=txtSCW_AvgSpeedUnit>

;!Raffke_jr - 2x removed w, align
	<TEXT    txtSCW_Distance     template=txt_sec textmodel_wstr="navigation.curr_speedcam.distance.number_text"  visiblemodel="navigation.curr_speedcam.distance.valid">
	<TEXT    txtSCW_DistanceUnit template=txt_sec textmodel_wstr="navigation.curr_speedcam.distance.unit_text"    visiblemodel="navigation.curr_speedcam.distance.valid">
	<ALIGNER xxx                 x=80  w=58 align="CENTER" control=txtSCW_Distance padding=4 control=txtSCW_DistanceUnit>

	<SPRITE xxx x=75 y=20 z=100 bmp="cock_infobg2.bmp" alpha=0 onrelease='run sc_ToggleSpeedcamSound'>
</layer>